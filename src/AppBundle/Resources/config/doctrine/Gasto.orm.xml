<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Gasto" table="gasto">
    <indexes>
      <index name="IDX_AE43DA141BB58C54" columns="orgaoSuperior_id"/>
      <index name="IDX_AE43DA14A5FAC5E3" columns="orgaoSubordinado_id"/>
      <index name="IDX_AE43DA148F9AC1B" columns="unidadeGestora_id"/>
      <index name="IDX_AE43DA145DBD78C5" columns="portador_nome"/>
      <index name="IDX_AE43DA1461E2C3B7" columns="favorecido_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="anoExtrato" type="smallint" column="anoExtrato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mesExtrato" type="smallint" column="mesExtrato" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nomeTransacao" type="string" column="nomeTransacao" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dataTransacao" type="object" column="dataTransacao" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="valorTransacao" type="decimal" column="valorTransacao" precision="10" scale="2" nullable="false"/>
    <many-to-one field="orgaoSuperior" target-entity="Orgao" fetch="LAZY">
      <join-columns>
        <join-column name="orgaoSuperior_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="portador" target-entity="Portador" fetch="LAZY">
      <join-columns>
        <join-column name="portador_nome" referenced-column-name="nomePortador"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="favorecido" target-entity="Favorecido" fetch="LAZY">
      <join-columns>
        <join-column name="favorecido_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="unidadeGestora" target-entity="UnidadeGestora" fetch="LAZY">
      <join-columns>
        <join-column name="unidadeGestora_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="orgaoSubordinado" target-entity="Orgao" fetch="LAZY">
      <join-columns>
        <join-column name="orgaoSubordinado_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
